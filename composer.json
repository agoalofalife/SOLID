{
    "name": "agoalofalife/solid",
    "description": "Examples and a description of the basic principles SOLID",
    "require": {
        "php": ">=7.1",
        "fzaninotto/faker": "^1.6@dev"
    },
    "keywords": [
        "solid",
        "oop",
        "php"
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.1",
        "mockery/mockery": "^0.9.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "agoalofalife",
            "email": "agoalofalife@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --stop-on-failure --no-coverage"
